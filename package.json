{
  "name": "elasticsearch-restful",
  "version": "0.1.0",
  "engines": {
    "node": ">=0.10"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "author": "Alexey Kucherenko <alexei.kucherenko@gmail.com>",
  "description": "A Node.js client to work with Elasticsearch Service RESTful API to create and manage your Elastic Stack deployments",
  "keywords": [
    "elasticsearch",
    "restful",
    "rest",
    "api",
    "deployments"
  ],
  "license": "MIT",
  "repository": "git@github.com:killmenot/elasticsearch-restful-js.git",
  "scripts": {
    "build": "rm -Rf ./dist && tsc -b",
    "test": "jest",
    "test:build": "jest --testRegex .spec.js",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest",
    "lint": "eslint --fix ./src/**/*.ts",
    "format": "prettier \"**/*.ts\" --ignore-path ./.prettierignore --write && git status",
    "dev": "ts-node -r tsconfig-paths/register src/dev.ts"
  },
  "dependencies": {
    "axios": "^0.21.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.21",
    "@types/node": "^14.14.35",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "codecov": "^3.8.1",
    "commander": "^7.2.0",
    "eslint": "^7.22.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.6",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts}",
      "!src/index.ts",
      "!**/test/**"
    ],
    "coverageDirectory": "./coverage"
  }
}
